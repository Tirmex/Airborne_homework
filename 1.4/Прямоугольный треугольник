#include <iostream>

using namespace std;

int main() {
    int a,b,c;
    
    cin >> a;
    cin >> b; 
    cin >> c;
    
    if ((a + b > c) && (a + c > b) && (b + c > a)){
        if ((a*a + b*b == c*c) || (a*a == b*b + c*c) || (a*a + c*c == b*b)){
            cout << "YES";
            
        } else {
            cout << "NO";
        }
         
    } else {
        cout << "UNDEFINED";
    }
    
    return 0;
}
